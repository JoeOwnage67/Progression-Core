<?xml version="1.0" encoding="utf-8" ?>
<Patch>

<Operation Class="PatchOperationFindMod">
        <mods>
            <li>SimpleStairs</li>
        </mods>
		
        <match Class="PatchOperationSequence">
            <!--<success>Always</success>-->
            <operations>

<li Class="PatchOperationAdd">
  <xpath>Defs/ThingDef[defName="Gomi_SimpleStair"]</xpath>
  <value>
	<researchPrerequisites>
			<li>VFET_Construction</li>
		</researchPrerequisites>
  </value>
</li>

<li Class="PatchOperationAdd">
  <xpath>Defs/ThingDef[defName="Gomi_Handrail"]</xpath>
  <value>
	<researchPrerequisites>
			<li>VFET_Construction</li>
		</researchPrerequisites>
  </value>
</li>

<li Class="PatchOperationAdd">
  <xpath>Defs/ThingDef[defName="Gomi_HighWall"]</xpath>
  <value>
	<researchPrerequisites>
			<li>VFET_Construction</li>
		</researchPrerequisites>
  </value>
</li>

<li Class="PatchOperationAdd">
  <xpath>Defs/ThingDef[defName="Gomi_FloorFence"]</xpath>
  <value>
	<researchPrerequisites>
			<li>VFET_Construction</li>
		</researchPrerequisites>
  </value>
</li>

</operations>
</match>
</Operation>

</Patch>